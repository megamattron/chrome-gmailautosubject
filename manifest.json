{
  "name": "GMail auto-subject",
  "version": "1.0",
  "description": "Autogenerated subjects for GMail.",
  "content_scripts" : [
    {
      "matches" : [
        "file:///*/test.html",
        "http://mail.google.com/*",
        "https://mail.google.com/*"
      ],
      "js" : ["sum.js", "jquery-1.6.2.js", "autosubject.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "permissions": [
    "http://api.flickr.com/"
  ]
}